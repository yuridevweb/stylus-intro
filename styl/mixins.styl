flex-center()
    display: flex
    justify-content: center
    align-content: center

cf()
    zoom 1
    &:after
    &:before
        content ""
        display table
    &after
        clear both

py(px = 100px)
    padding: px 0px

px(px)
    padding 0px px


test(var)
    margin-top unit(var,'px')
    if var > 10
        margin-bottom unit(var, 'px')
